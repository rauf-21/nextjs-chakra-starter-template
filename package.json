    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/montserrat": "^4.5.12",
    "@prisma/client": "^4.3.1",
    "clsx": "^1.2.1",
    "framer-motion": "^6",
    "next": "12.3.1",
    "next-auth": "^4.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.21",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
